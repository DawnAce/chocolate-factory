[
  {
    "type": "frontend",
    "name": "Breadcrumb",
    "tagName": "<el-breadcrumb>",
    "description": "显示当前页面的路径，快速返回之前的任意页面。",
    "properties": [
      {
        "parameter": "separator",
        "description": "分隔符",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "斜杠'/"
      },
      {
        "parameter": "separator-class",
        "description": "图标分隔符 class",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      }
    ],
    "examples": [
      {
        "question": "基础用法",
        "answer": "<el-breadcrumb separator=\"/\">\n  <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n  <el-breadcrumb-item><a href=\"/\">活动管理</a></el-breadcrumb-item>\n  <el-breadcrumb-item>活动列表</el-breadcrumb-item>\n  <el-breadcrumb-item>活动详情</el-breadcrumb-item>\n</el-breadcrumb>"
      },
      {
        "question": "图标分隔符",
        "answer": "<el-breadcrumb separator-class=\"el-icon-arrow-right\">\n  <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n  <el-breadcrumb-item>活动管理</el-breadcrumb-item>\n  <el-breadcrumb-item>活动列表</el-breadcrumb-item>\n  <el-breadcrumb-item>活动详情</el-breadcrumb-item>\n</el-breadcrumb>"
      }
    ]
  },
  {
    "type": "frontend",
    "name": "Card",
    "tagName": "<el-card>",
    "description": "将信息聚合在卡片容器中展示。",
    "properties": [
      {
        "parameter": "header",
        "description": "设置 header，也可以通过 slot#header 传入 DOM",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "body-style",
        "description": "设置 body 的样式",
        "type": "object",
        "optionalValue": "—",
        "defaultValue": "{ padding: '20px' }"
      },
      {
        "parameter": "shadow",
        "description": "设置阴影显示时机",
        "type": "string",
        "optionalValue": "always / hover / never",
        "defaultValue": "always"
      }
    ],
    "examples": [
      {
        "question": "基础用法",
        "answer": "<el-card class=\"box-card\">\n  <div slot=\"header\" class=\"clearfix\">\n    <span>卡片名称</span>\n    <el-button style=\"float: right; padding: 3px 0\" type=\"text\">操作按钮</el-button>\n  </div>\n  <div v-for=\"o in 4\" :key=\"o\" class=\"text item\">\n    {{'列表内容 ' + o }}\n  </div>\n</el-card>"
      },
      {
        "question": "简单卡片",
        "answer": "<el-card class=\"box-card\">\n  <div v-for=\"o in 4\" :key=\"o\" class=\"text item\">\n    {{'列表内容 ' + o }}\n  </div>\n</el-card>"
      },
      {
        "question": "带图片",
        "answer": "<el-row>\n  <el-col :span=\"8\" v-for=\"(o, index) in 2\" :key=\"o\" :offset=\"index > 0 ? 2 : 0\">\n    <el-card :body-style=\"{ padding: '0px' }\">\n      <img src=\"https://shadow.elemecdn.com/app/element/hamburger.9cf7b091-55e9-11e9-a976-7f4d0b07eef6.png\" class=\"image\">\n      <div style=\"padding: 14px;\">\n        <span>好吃的汉堡</span>\n        <div class=\"bottom clearfix\">\n          <time class=\"time\">{{ currentDate }}</time>\n          <el-button type=\"text\" class=\"button\">操作按钮</el-button>\n        </div>\n      </div>\n    </el-card>\n  </el-col>\n</el-row>"
      },
      {
        "question": "卡片阴影",
        "answer": "<el-row :gutter=\"12\">\n  <el-col :span=\"8\">\n    <el-card shadow=\"always\">\n      总是显示\n    </el-card>\n  </el-col>\n  <el-col :span=\"8\">\n    <el-card shadow=\"hover\">\n      鼠标悬浮时显示\n    </el-card>\n  </el-col>\n  <el-col :span=\"8\">\n    <el-card shadow=\"never\">\n      从不显示\n    </el-card>\n  </el-col>\n</el-row>"
      }
    ]
  },
  {
    "type": "frontend",
    "name": "Dialog",
    "tagName": "<el-dialog>",
    "description": "一种常见的用户界面元素，用于在当前页面的上层显示一块浮动的区域，通常用于展示一些重要信息、交互或者需要用户确认的操作。",
    "properties": [
      {
        "parameter": "visible",
        "description": "是否显示 Dialog，支持 .sync 修饰符",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "title",
        "description": "Dialog 的标题，也可通过具名 slot （见下表）传入",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "width",
        "description": "Dialog 的宽度",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "50%"
      },
      {
        "parameter": "fullscreen",
        "description": "是否为全屏 Dialog",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "top",
        "description": "Dialog CSS 中的 margin-top 值",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "15vh"
      },
      {
        "parameter": "modal",
        "description": "是否需要遮罩层",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "true"
      },
      {
        "parameter": "modal-append-to-body",
        "description": "遮罩层是否插入至 body 元素上，若为 false，则遮罩层会插入至 Dialog 的父元素上",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "true"
      },
      {
        "parameter": "append-to-body",
        "description": "Dialog 自身是否插入至 body 元素上。嵌套的 Dialog 必须指定该属性并赋值为 true",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "lock-scroll",
        "description": "是否在 Dialog 出现时将 body 滚动锁定",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "true"
      },
      {
        "parameter": "custom-class",
        "description": "Dialog 的自定义类名",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "close-on-click-modal",
        "description": "是否可以通过点击 modal 关闭 Dialog",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "true"
      },
      {
        "parameter": "close-on-press-escape",
        "description": "是否可以通过按下 ESC 关闭 Dialog",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "true"
      },
      {
        "parameter": "show-close",
        "description": "是否显示关闭按钮",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "true"
      },
      {
        "parameter": "before-close",
        "description": "关闭前的回调，会暂停 Dialog 的关闭",
        "type": "function(done)，done 用于关闭 Dialog",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "center",
        "description": "是否对头部和底部采用居中布局",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "destroy-on-close",
        "description": "关闭时销毁 Dialog 中的元素",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      }
    ],
    "events": [
      {
        "eventName": "open",
        "description": "Dialog 打开的回调",
        "callbackParameter": "—"
      },
      {
        "eventName": "opened",
        "description": "Dialog 打开动画结束时的回调",
        "callbackParameter": "—"
      },
      {
        "eventName": "close",
        "description": "Dialog 关闭的回调",
        "callbackParameter": "—"
      },
      {
        "eventName": "closed",
        "description": "Dialog 关闭动画结束时的回调",
        "callbackParameter": "—"
      }
    ],
    "examples": [
      {
        "question": "Dialog 弹出一个对话框，适合需要定制性更大的场景。",
        "answer": "<el-dialog\n  title=\"提示\"\n  :visible.sync=\"dialogVisible\"\n  width=\"30%\"\n  :before-close=\"handleClose\">\n  <span>这是一段信息</span>\n  <span slot=\"footer\" class=\"dialog-footer\">\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\n    <el-button type=\"primary\" @click=\"dialogVisible = false\">确 定</el-button>\n  </span>\n</el-dialog>\n"
      },
      {
        "question": "Dialog 组件的内容可以是任意的，甚至可以是表格或表单，下面是应用了 Element Table 和 Form 组件的两个样例。",
        "answer": "\n<el-dialog title=\"收货地址\" :visible.sync=\"dialogTableVisible\">\n  <el-table :data=\"gridData\">\n    <el-table-column property=\"date\" label=\"日期\" width=\"150\"></el-table-column>\n    <el-table-column property=\"name\" label=\"姓名\" width=\"200\"></el-table-column>\n    <el-table-column property=\"address\" label=\"地址\"></el-table-column>\n  </el-table>\n</el-dialog>\n"
      }
    ]
  },
  {
    "type": "frontend",
    "name": "PageHeader",
    "tagName": "<el-page-header>",
    "description": "如果页面的路径比较简单，推荐使用页头组件而非面包屑组件。",
    "properties": [
      {
        "parameter": "title",
        "description": "标题",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "返回"
      },
      {
        "parameter": "content",
        "description": "内容",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      }
    ],
    "events": [
      {
        "eventName": "back",
        "description": "点击左侧区域触发",
        "callbackParameter": "—"
      }
    ],
    "examples": [
      {
        "question": "基础",
        "answer": "<el-page-header @back=\"goBack\" content=\"详情页面\">\n</el-page-header>\n\n<script>\n  export default {\n    methods: {\n      goBack() {\n        console.log('go back');\n      }\n    }\n  }\n</script>"
      }
    ]
  },
  {
    "type": "frontend",
    "name": "Pagination",
    "tagName": "<el-pagination>",
    "description": "分页组件用于展示数据分页，并提供用户切换页面的功能。",
    "properties": [
      {
        "parameter": "small",
        "description": "是否使用小型分页样式",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "background",
        "description": "是否为分页按钮添加背景色",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "page-size",
        "description": "每页显示条目个数，支持 .sync 修饰符",
        "type": "number",
        "optionalValue": "—",
        "defaultValue": "10"
      },
      {
        "parameter": "total",
        "description": "总条目数",
        "type": "number",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "page-count",
        "description": "总页数，total 和 page-count 设置任意一个就可以达到显示页码的功能；如果要支持 page-sizes 的更改，则需要使用 total 属性",
        "type": "number",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "pager-count",
        "description": "页码按钮的数量，当总页数超过该值时会折叠",
        "type": "number",
        "optionalValue": "大于等于 5 且小于等于 21 的奇数",
        "defaultValue": "7"
      },
      {
        "parameter": "current-page",
        "description": "当前页数，支持 .sync 修饰符",
        "type": "number",
        "optionalValue": "—",
        "defaultValue": "1"
      },
      {
        "parameter": "layout",
        "description": "组件布局，子组件名用逗号分隔",
        "type": "string",
        "optionalValue": "sizes, prev, pager, next, jumper, ->, total, slot",
        "defaultValue": "'prev, pager, next, jumper, ->, total'"
      },
      {
        "parameter": "page-sizes",
        "description": "每页显示个数选择器的选项设置",
        "type": "number[]",
        "optionalValue": "—",
        "defaultValue": "[10, 20, 30, 40, 50, 100]"
      },
      {
        "parameter": "popper-class",
        "description": "每页显示个数选择器的下拉框类名",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "prev-text",
        "description": "替代图标显示的上一页文字",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "next-text",
        "description": "替代图标显示的下一页文字",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "disabled",
        "description": "是否禁用",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "hide-on-single-page",
        "description": "只有一页时是否隐藏",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "-"
      }
    ],
    "events": [
      {
        "eventName": "size-change",
        "description": "pageSize 改变时会触发",
        "callbackParameter": "每页条数"
      },
      {
        "eventName": "current-change",
        "description": "currentPage 改变时会触发",
        "callbackParameter": "当前页"
      },
      {
        "eventName": "prev-click",
        "description": "用户点击上一页按钮改变当前页后触发",
        "callbackParameter": "当前页"
      },
      {
        "eventName": "next-click",
        "description": "用户点击下一页按钮改变当前页后触发",
        "callbackParameter": "当前页"
      }
    ],
    "examples": [
      {
        "question": "基础用法",
        "answer": " <el-pagination\n    layout=\"prev, pager, next\"\n    :total=\"50\">\n  </el-pagination>"
      },
      {
        "question": "设置最大页码按钮数",
        "answer": "<el-pagination\n  :page-size=\"20\"\n  :pager-count=\"11\"\n  layout=\"prev, pager, next\"\n  :total=\"1000\">\n</el-pagination>"
      }
    ]
  },
  {
