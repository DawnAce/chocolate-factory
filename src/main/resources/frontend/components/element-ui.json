        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "disable-transitions",
        "description": "是否禁用渐变动画",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "hit",
        "description": "是否有边框描边",
        "type": "boolean",
        "optionalValue": "—",
        "defaultValue": "false"
      },
      {
        "parameter": "color",
        "description": "背景色",
        "type": "string",
        "optionalValue": "—",
        "defaultValue": "—"
      },
      {
        "parameter": "size",
        "description": "尺寸",
        "type": "string",
        "optionalValue": "medium / small / mini",
        "defaultValue": "—"
      },
      {
        "parameter": "effect",
